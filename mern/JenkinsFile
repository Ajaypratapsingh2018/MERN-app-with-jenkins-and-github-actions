pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                git credentialsId: 'github-token', url: 'https://github.com/Ajaypratapsingh2018/MERN-app-with-jenkins-and-github-actions.git'
            }
        }
        stage('Build') {
            steps {
                echo 'Run your build here...'
            }
        }
        stage('Test') {
            steps {
                echo 'Run your tests here...'
            }
        }
    }
}
