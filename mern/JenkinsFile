pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                git credentialsId: 'github-token', url: 'https://github.com/<your-username>/<repo>.git'
            }
        }
        stage('Build') {
            steps {
                echo 'Run your build here...'
            }
        }
        stage('Test') {
            steps {
                echo 'Run your tests here...'
            }
        }
    }
}
